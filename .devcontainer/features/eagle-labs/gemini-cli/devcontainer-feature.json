{
  "id": "gemini-cli",
  "version": "1.0.0",
  "name": "Gemini CLI (via npm)",
  "documentationURL": "https://geminicli.com/docs/",
  "description": "Installs Google's Gemini CLI for AI-powered terminal access",
  "options": {
    "version": {
      "default": "latest",
      "description": "Select the version to install.",
      "proposals": [
        "latest"
      ],
      "type": "string"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Google.genai-code-assist"
      ]
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/node"
  ]
}
